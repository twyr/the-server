{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc"]
	},
	"source": {
		"include": ["source"],
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"opts": {
		"template": "./node_modules/@appku/jsdoc-template",
		"destination": "./jsdoc_default",
		"readme": "./README.md",
		"package": "./package.json",
		"encoding": "utf8",
		"recurse": true,
		"verbose": true,
		"prism-theme": "prism-default"
	},
	"plugins": ["plugins/markdown", "plugins/summarize"],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"metadata": {
		"title": "The Server by Twy'r"
	}
}
